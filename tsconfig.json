{
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "target": "es5",
    "lib": ["dom", "esnext"],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "jsx": "react",
    "declaration": true,
    "downlevelIteration": true,
    "skipLibCheck": true
  },
  "include": ["./src"]
}
